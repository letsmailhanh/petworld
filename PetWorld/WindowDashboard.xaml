<Window x:Class="PetWorld.WindowDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetWorld"
        mc:Ignorable="d"
        Title="WindowDashboard" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Click="btnUserClick">User</Button>
            <Button Click="btnPetClick">Pet</Button>
            <Button Click="btnProductClick">Product</Button>
            <Button Click="btnOrderClick">Order</Button>
        </StackPanel>
    </Grid>
</Window>
